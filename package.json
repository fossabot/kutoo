{
  "name": "kutoo",
  "version": "0.0.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./dist/cli.js",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "commander": "^5.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "got": "^10.7.0",
    "image-size": "^0.8.3",
    "lodash": "^4.17.15",
    "m3u8-parser": "^4.4.2",
    "m3u8stream": "^0.6.5",
    "pdfkit": "^0.11.0",
    "sanitize-filename": "^1.6.3",
    "tough-cookie": "^4.0.0"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "@types/cheerio": "^0.22.17",
    "@types/fluent-ffmpeg": "^2.1.14",
    "@types/jest": "^25.1.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.0",
    "@types/pdfkit": "^0.10.5",
    "@types/tough-cookie": "^4.0.0",
    "@types/webpack": "^4.41.10",
    "@typescript-eslint/eslint-plugin": "2",
    "@typescript-eslint/parser": "^2.26.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^15.0.1",
    "eslint-plugin-import": "2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "eslint-plugin-standard": "4",
    "jest": "^25.2.7",
    "nock": "^12.0.3",
    "np": "^6.2.0",
    "pkg": "^4.4.5",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "clean": "del-cli dist release coverage",
    "lint": "eslint --ext .js,.ts . --fix",
    "test": "yarn lint && tsc --noEmit && jest",
    "start": "node -r ts-node/register -r tsconfig-paths/register",
    "start:cli": "yarn start src/cli.ts",
    "prepare": "yarn build",
    "postpublish": "yarn package:all",
    "release": "np",
    "build": "yarn clean && webpack",
    "package:all": "yarn package:linux && yarn package:win && yarn package:macos",
    "package:linux": "pkg --targets node12-linux --output release/kutoo-linux release/cli.bundle.js",
    "package:win": "pkg --targets node12-win --output release/kutoo-win release/cli.bundle.js",
    "package:win:x86": "pkg --targets node12-win-x86 --output release/kutoo-win-x86 release/cli.bundle.js",
    "package:macos": "pkg --targets node12-macos --output release/kutoo-macos release/cli.bundle.js",
    "dist": "yarn build && yarn package:all"
  },
  "files": [
    "dist/",
    "LICENSE"
  ],
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "keywords": [
    "anime",
    "crunchyroll",
    "download",
    "manga",
    "mangadex",
    "nhentai",
    "scraping"
  ],
  "repository": "https://github.com/FedericoMorrone/kutoo.git",
  "author": "Federico Morrone <flickmor@gmail.com>"
}
